              <thead>  
                <tr>  
                  <th>brokerName</th>
				  <th>namesrvAddr</th>
				  <th>wipeTopicCount</th>
                </tr>  
              </thead>  
              <tbody> 
				#foreach($map in $list)
					<tr>
						<td>$map.brokerName</td>
						<td>$map.namesrvAddr</td>
						<td>$map.wipeTopicCount</td>
					</tr>
				#end
              </tbody>
        </table>
	#end
		
   #include("foot.vm")
      </div>
  </body>
</html>
