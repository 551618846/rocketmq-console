<!DOCTYPE html>
<html lang="en">
<head>
  #include("head.vm")
</head>
  
  <body>
  <!-- head-->
  #parse("body_1.vm")
   <div class="container theme-showcase"><!-- /container  -->
	  <form class="navbar-form navbar-left" role="search" action="wipeWritePerm.do" method="post">
          brokerName:
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="brokerName" value="$!brokerName" name="brokerName">
