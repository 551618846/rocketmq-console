<!DOCTYPE html>
<html lang="en">
<head>
  #include("head.vm")
</head>
  
  <body>
  <!-- head-->
  #parse("body_1.vm")
   <div class="container theme-showcase"><!-- /container  -->
	  <form class="navbar-form navbar-left" role="search" action="queryMsgByKey.do" method="post">
		  topicName:
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="topicName" value="$!topicName" name="topicName">
          </div>
		   msgKey:
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="msgKey" value="$!msgKey" name="msgKey">
          </div>
		   fallbackHours:
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="fallbackHours" value="$!fallbackHours" name="fallbackHours">
          </div>
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="key" value="$!key" name="key">
          </div>
		  value:
		  <div class="form-group">
            <input type="text" class="form-control" placeholder="value" value="$!value" name="value">
          </div>
		  <button type="submit" class="btn btn-default">Submit</button>
      </form>
   <br>
   #if($result)
		<table class="table table-bordered table-hover">  
			<thead>
					<tr>
				         #foreach($head in $result.thead)
						     <td>$head</td>
						 #end
					</tr>
            </thead>
              <tbody> 
				#foreach($tr in $result.tbodyData)
					<tr>
						#foreach($td in $tr)
						  <td>$td</td>
						#end
					</tr>
				#end
              </tbody>
        </table>
	#end
   #include("foot.vm")
      </div>
  </body>
</html>
